<script type="text/javascript">
function path()
{
  var args = arguments,
      result = []
      ;
       
  for(var i = 0; i < args.length; i++)
      result.push(args[i].replace('@', '/public/js/syntaxhighlighter/'));
       
  return result
};
 
SyntaxHighlighter.autoloader.apply(null, path(
  'bash                   @shBrushBash.js',
  'cpp                    @shBrushCpp.js',
  'c#                     @shBrushCSharp.js',
  'css                    @shBrushCss.js',
  'diff                   @shBrushDiff.js',
  'java                   @shBrushJava.js',
  'javascript             @shBrushJScript.js',
  'perl                   @shBrushPerl.js',
  'php                    @shBrushPhp.js',
  'plain                  @shBrushPlain.js',
  'python                 @shBrushPython.js',
  'ruby                   @shBrushRuby.js',
  'sql                    @shBrushSql.js',
  'xml                    @shBrushXml.js'
));
 
SyntaxHighlighter.all();
</script>
